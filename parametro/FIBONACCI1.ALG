Algoritmo "Fibonacci1"

Var
   N1,N2,c,N3:inteiro
Procedimento soma(var A, B, D:inteiro)
inicio
      D <- A + B
      A <- B
      B <- D

fimprocedimento

Inicio
      N1 <- 0
      N2 <- 1
      para c <- 1 ate 10 faca
      se c = 3 entao
      escreva(" 1")
      fimse
      escreva(N3)
      soma(N1,N2,N3)
      
      
      fimpara

Fimalgoritmo